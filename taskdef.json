  "volumes": [
    {
      "fsxWindowsFileServerVolumeConfiguration": null,
      "efsVolumeConfiguration": {
        "transitEncryptionPort": null,
        "fileSystemId": "fs-0b81e6fb963f8bb7f",
        "authorizationConfig": {
          "iam": "DISABLED",
          "accessPointId": "fsap-045ea23c25b53dcc5"
        },
        "transitEncryption": "ENABLED",
