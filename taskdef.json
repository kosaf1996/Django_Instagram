{
  "executionRoleArn": "arn:aws:iam::583977559633:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/django-app-def",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "cpu": 256,
      "readonlyRootFilesystem": true,
      "environment": [],
      "memory": null,
      "memoryReservation": 512,
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "name": "app"
    }
  ],
      "volumes": [
        {
            "name": "project",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-00d8587d8df7bbd52",
                "rootDirectory": "/django/Django_Instagram/media"
            }
        }
    ],
  "memory": "1024",
  "taskRoleArn": null,
  "family": "django-app-def",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "512"
}
